# LLM Configuration
LLM_PROVIDER=ollama  # ollama or openai
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.1:8b
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4
OPENAI_BASE_URL=https://api.openai.com/v1  # Can be changed for compatible APIs

# ClickHouse Configuration
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=8123
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=
CLICKHOUSE_DATABASE=sdlc_agents

# Azure DevOps Configuration
ADO_ORGANIZATION=your-org
ADO_PROJECT=your-project
ADO_PAT=your-personal-access-token
ADO_BASE_URL=https://dev.azure.com

# Git Configuration
GIT_USER_NAME=SDLC Agent
GIT_USER_EMAIL=sdlc-agent@example.com

# Maven Configuration
MAVEN_HOME=/usr/share/maven
MAVEN_OPTS=-Xmx2g

# Agent Configuration
MAX_RETRIES=3
BUILD_TIMEOUT=600
AGENT_MEMORY_RETENTION_DAYS=90

# Logging
LOG_LEVEL=INFO
LOG_FILE=sdlc_agents.log
